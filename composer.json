{
    "name" : "kirago/laravel-business-core",
    "description" : "A Laravel business core multi-tenancies management data structure : orders, products, invoices, payments and more ...",
    "keywords": [
        "laravel","kirago","business","models"
      ],
    "homepage" : "https://github.com/jsimo237/laravel-business-core",
    "version": "1.0",
    "support" : {
        "issues" : "https://github.com/jsimo237/laravel-business-core/issues"
    },
    "authors" : [
        {
            "name" : "Simo Joel",
            "email" : "joelsimooverride@gmail.com",
            "role": "Developper",
            "homepage": "https://github.com/jsimo237"
        }
    ],
    "license" : "MIT",
    "require" : {
        "php": "^7.3|^8",
        "ext-json": "*",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "axn/laravel-eloquent-authorable": "*",
        "barryvdh/laravel-dompdf": "*",
        "calebporzio/sushi": "*",
        "laravel/sanctum": "*",
        "spatie/laravel-medialibrary": "*",
        "spatie/laravel-permission": "*",
        "spatie/laravel-activitylog": "*",
        "spatie/laravel-translatable": "*",
        "staudenmeir/eloquent-has-many-deep": "*",
        "cviebrock/eloquent-sluggable": "*",
        "stevebauman/location": "*",
        "spatie/valuestore": "*",
        "laravel-notification-channels/webpush": "*",
        "spatie/laravel-sluggable": "*",
        "laravel-json-api/laravel": "*",
        "simplesoftwareio/simple-qrcode": "*"

    },
    "require-dev": {
        "kirago/laravel-xpeedy-service": "dev-main"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload" : {
        "files": [
            "src/helpers.php"
        ],
        "psr-4" : {
            "Kirago\\BusinessCore\\" : "src/",
            "Kirago\\BusinessCore\\Database\\Factories\\": "database/factories/",
            "Kirago\\BusinessCore\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Kirago\\BusinessCore\\Providers\\BcServiceProvider"
            ]
        }
    }
}